      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_WkIlEDGAEem4RqP6_nGyvA" specification="_MhQL4DF1Eem4RqP6_nGyvA">
        <language>C++</language>
        <body>using namespace std;&#xD;
char charcol1 = Start[0];&#xD;
int startcol;&#xD;
switch(charcol1){&#xD;
    case 'A': startcol = 0; break;&#xD;
    case 'B': startcol = 1; break;&#xD;
    case 'C': startcol = 2; break;&#xD;
    case 'D': startcol = 3; break;&#xD;
    case 'E': startcol = 4; break;&#xD;
    case 'F': startcol = 5; break;&#xD;
    case 'G': startcol = 6; break;&#xD;
    case 'H': startcol = 7; break;&#xD;
}&#xD;
&#xD;
return startcol;&#xD;
</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_F8Ln8DyPEemdYIOBm08Mew" specification="_oJ98YDyOEemdYIOBm08Mew">
        <language>C++</language>
        <body>bool moveValid = false;&#xD;
if(ChessBoard[startRow][startCol] == 0){&#xD;
	return moveValid;&#xD;
}&#xD;
&#xD;
if(ChessBoard[startRow][startCol] &lt; 0){&#xD;
	return moveValid;&#xD;
}&#xD;
&#xD;
//Player selected a pawn&#xD;
if(ChessBoard[startRow][startCol] == 1){&#xD;
	//Moving one step into an empty space&#xD;
	if(startCol == endCol &amp;&amp; startRow - endRow ==1 &amp;&amp; ChessBoard[endRow][endCol] == 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	//Capturing a piece on left diagonal&#xD;
	if(startCol - endCol == 1 &amp;&amp; startRow - endRow == 1 &amp;&amp; ChessBoard[endRow][endCol] &lt; 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	//Capturing a piece on right diagonal&#xD;
	if(endCol - startCol == 1 &amp;&amp; startRow - endRow == 1 &amp;&amp; ChessBoard[endRow][endCol] &lt; 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	return moveValid = false;&#xD;
}&#xD;
&#xD;
//Player selected a rook&#xD;
if(ChessBoard[startRow][startCol] == 2){&#xD;
	//Moving into an empty cell in the same column&#xD;
	if(startCol == endCol &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	//Moving into an empty cell in the same row&#xD;
	if(startRow == endRow &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	return moveValid;&#xD;
}&#xD;
//Player selected a knight&#xD;
if(ChessBoard[startRow][startCol] == 3){&#xD;
	return moveValid;&#xD;
}&#xD;
//Player selected a bishop&#xD;
if(ChessBoard[startRow][startCol] == 4){&#xD;
&#xD;
&#xD;
	return moveValid;&#xD;
}&#xD;
//Player selected the Queen&#xD;
if(ChessBoard[startRow][startCol] == 5){&#xD;
	if(startCol == endCol &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	if(startRow == endRow &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	return moveValid;&#xD;
}&#xD;
//Player selected the King&#xD;
if(ChessBoard[startRow][startCol] == 6){&#xD;
	//Moving one step in the same column&#xD;
	if(startCol == endCol &amp;&amp; abs(startRow - endRow) ==1 &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	//Moving one step in the same row&#xD;
	if(startRow == endRow &amp;&amp; abs(startCol - endCol) ==1 &amp;&amp; ChessBoard[endRow][endCol] &lt;= 0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	//Moving one step along any diagonal&#xD;
	if(abs(startCol - endCol) == 1 &amp;&amp; abs(startRow - endRow) == 1 &amp;&amp; ChessBoard[endRow][endCol] &lt;=0){&#xD;
		return moveValid = true;&#xD;
	}&#xD;
	return moveValid;&#xD;
}&#xD;
 </body>
      </ownedBehavior>
